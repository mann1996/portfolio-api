<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >
<hibernate-reverse-engineering>
	<type-mapping>
		<sql-type jdbc-type="TIMESTAMP" hibernate-type="timestamp"
			length="19" not-null="true">
		</sql-type>
		<sql-type jdbc-type="BIT" hibernate-type="boolean"
			not-null="true">
		</sql-type>
		<sql-type jdbc-type="LONGVARCHAR" hibernate-type="string"
			not-null="true">
		</sql-type>
		<sql-type jdbc-type="VARCHAR" hibernate-type="string"
			not-null="true">
		</sql-type>
		<sql-type jdbc-type="DATE" hibernate-type="date" length="10"
			not-null="true">
		</sql-type>
	</type-mapping>
	<table-filter match-schema="portfolio" match-name=".*"></table-filter>
	<table-filter match-schema="phpmyadmin" match-name=".*"
		exclude="true">
	</table-filter>
	<table-filter match-schema="mysql" match-name=".*"
		exclude="true"></table-filter>
	<table-filter match-schema="blugaa" match-name=".*"
		exclude="true">
	</table-filter>
</hibernate-reverse-engineering>
